@prefix rdfc: <https://w3id.org/rdf-connect#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

rdfc:LdesClient rdfc:jsImplementationOf rdfc:Processor;
  rdfs:label "ldes client";
  rdfs:comment "An LDES client that can read a stream of members from an LDES.";
  rdfc:file <./dist/lib/rdfc-processor.js>;
  rdfc:class "LDESClientProcessor";
  rdfc:entrypoint <./>.

[ ] a sh:NodeShape;
  sh:targetClass rdfc:FetchConfig;
  sh:property [
    sh:class rdfc:AuthConfig;
    sh:path rdfc:auth;
    sh:name "auth";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:integer;
    sh:path rdfc:concurrent;
    sh:name "concurrent";
    sh:maxCount 1;
  ], [
    sh:class rdfc:FetchRetry;
    sh:path rdfc:retry;
    sh:name "retry";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:boolean;
    sh:path rdfc:safe;
    sh:name "safe";
    sh:maxCount 1;
  ].

[ ] a sh:NodeShape;
  sh:targetClass rdfc:FetchRetry;
  sh:property [
    sh:datatype xsd:integer;
    sh:path rdfc:code;
    sh:name "codes";
  ], [
    sh:datatype xsd:integer;
    sh:path rdfc:maxRetry;
    sh:name "maxRetries";
    sh:maxCount 1;
  ].

[ ] a sh:NodeShape;
  sh:targetClass rdfc:AuthConfig;
  sh:property [
    sh:datatype xsd:string;
    sh:path rdfc:type;
    sh:name "type";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:string;
    sh:path rdfc:auth;
    sh:name "auth";
    sh:maxCount 1;
  ].

[ ] a sh:NodeShape;
  sh:targetClass rdfc:LdesClient;
  sh:property [
    sh:datatype xsd:string;
    sh:path rdfc:url;
    sh:name "url";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:class rdfc:Writer;
    sh:path rdfc:output;
    sh:name "output";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:datatype xsd:dateTime;
    sh:path rdfc:before;
    sh:name "before";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:dateTime;
    sh:path rdfc:after;
    sh:name "after";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:string;
    sh:path rdfc:ordered;
    sh:name "ordered";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:boolean;
    sh:path rdfc:follow;
    sh:name "follow";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:integer;
    sh:path rdfc:interval;
    sh:name "pollInterval";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:string;
    sh:path rdfc:shapeFile;
    sh:name "shapeFile";
    sh:minCount 0;
    sh:maxCount 1;
  ], [
    sh:datatype xsd:boolean;
    sh:path rdfc:noShape;
    sh:name "noShape";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:string;
    sh:path rdfc:savePath;
    sh:name "savePath";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:boolean;
    sh:path rdfc:loose;
    sh:name "loose";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:boolean;
    sh:path rdfc:urlIsView;
    sh:name "urlIsView";
    sh:maxCount 1;
  ], [
    sh:class rdfc:FetchConfig;
    sh:path rdfc:fetch;
    sh:name "fetchConfig";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:string;
    sh:path rdfc:conditionFile;
    sh:name "conditionFile";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:boolean;
    sh:path rdfc:materialize;
    sh:name "materialize";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:boolean;
    sh:path rdfc:startFresh;
    sh:name "startFresh";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:boolean;
    sh:path rdfc:lastVersionOnly;
    sh:name "lastVersionOnly";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:string;
    sh:path rdfc:streamId;
    sh:name "streamId";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:boolean;
    sh:path rdfc:sdsify;
    sh:name "sdsify";
    sh:maxCount 1;
  ].

