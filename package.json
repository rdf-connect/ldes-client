{
  "name": "ldes-client",
  "description": "This package provides common tooling to work with LDESes.",
  "version": "0.3.0",
  "main": "dist/lib/client.js",
  "exports": {
    "import": "./dist/lib/client.js",
    "require": "./dist/lib/client.cjs"
  },
  "type": "module",
  "bin": {
    "ldes-client": "dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias && rollup -c",
    "test": "vitest run --coverage --coverage.include lib --coverage.exclude dist",
    "test:browser": "vitest run -c vitest.browser.config.ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/**/*",
    "shapes/**/*",
    "dist/bin/**/*",
    "processor.ttl"
  ],
  "directories": {
    "lib": "lib",
    "bin": "bin",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rdf-connect/ldes-client"
  },
  "author": "Arthur Vercruysse",
  "license": "MIT",
  "dependencies": {
    "@treecg/types": "^0.4.6",
    "commander": "^14.0.2",
    "extract-cbd-shape": "^0.1.14",
    "heap-js": "^2.7.1",
    "level": "^10.0.0",
    "n3": "^2.0.1",
    "rdf-data-factory": "^2.0.2",
    "rdf-dereference": "^4.0.0",
    "rdf-lens": "^1.3.6",
    "rdf-stores": "^2.1.1",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/static": "^9.0.0",
    "@grpc/grpc-js": "^1.14.3",
    "@rdfc/js-runner": "^3.0.1",
    "@rdfc/proto": "^0.1.2",
    "@rdfjs/types": "^2.0.1",
    "@types/n3": "^1.26.1",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "benchmark": "^2.1.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "fastify": "^5.7.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "playwright": "^1.58.0",
    "prettier": "^3.8.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.18"
  },
  "browser": {
    "fs": false,
    "fs/promises": false,
    "path": false,
    "os": false
  },
  "optionalDependencies": {
    "@esbuild/darwin-arm64": "^0.27.2",
    "@rollup/rollup-linux-x64-gnu": "^4.56.0"
  }
}
