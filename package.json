{
  "name": "ldes-client",
  "description": "This package provides common tooling to work with LDESes.",
  "version": "0.1.8",
  "main": "dist/lib/client.js",
  "exports": {
    "import": "./dist/lib/client.js",
    "require": "./dist/lib/client.cjs"
  },
  "type": "module",
  "bin": {
    "ldes-client": "dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias && rollup ./dist/lib/client.js --file ./dist/lib/client.cjs --format cjs",
    "test": "vitest run --coverage --coverage.include lib",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/**/*",
    "shapes/**/*",
    "dist/bin/**/*",
    "processor.ttl"
  ],
  "directories": {
    "lib": "lib",
    "bin": "bin",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rdf-connect/ldes-client"
  },
  "author": "Arthur Vercruysse",
  "license": "MIT",
  "dependencies": {
    "@treecg/types": "^0.4.6",
    "@types/debug": "^4.1.12",
    "commander": "^14.0.0",
    "debug": "^4.4.1",
    "extract-cbd-shape": "^0.1.14",
    "heap-js": "^2.6.0",
    "n3": "^1.26.0",
    "rdf-data-factory": "^2.0.2",
    "rdf-dereference": "^4.0.0",
    "rdf-lens": "^1.3.6",
    "rdf-stores": "^2.1.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@fastify/static": "^8.2.0",
    "@rdfc/js-runner": "^1.0.0",
    "@rdfjs/types": "^2.0.1",
    "@types/n3": "^1.26.0",
    "@types/node": "^22.18.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^3.2.4",
    "benchmark": "^2.1.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "fastify": "^5.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "browser": {
    "fs": false,
    "fs/promises": false,
    "path": false,
    "os": false
  },
  "optionalDependencies": {
    "@esbuild/darwin-arm64": "^0.25.9",
    "@rollup/rollup-linux-x64-gnu": "^4.50.0"
  }
}
