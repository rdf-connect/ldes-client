{
  "name": "ldes-client",
  "description": "This package provides common tooling to work with LDESes.",
  "version": "0.0.16",
  "main": "dist/lib/client.js",
  "exports": {
    "import": "./dist/lib/client.js",
    "require": "./dist/lib/client.cjs"
  },
  "type": "module",
  "bin": {
    "ldes-client": "dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias && rollup ./dist/lib/client.js --file ./dist/lib/client.cjs --format cjs",
    "test": "vitest run --coverage --coverage.include lib"
  },
  "files": [
    "dist/**/*",
    "shapes/**/*",
    "dist/bin/**/*",
    "processor.ttl"
  ],
  "directories": {
    "lib": "lib",
    "bin": "bin",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rdf-connect/ldes-client"
  },
  "author": "Arthur Vercruysse",
  "license": "MIT",
  "dependencies": {
    "@treecg/types": "^0.4.6",
    "@types/debug": "^4.1.12",
    "commander": "^12.1.0",
    "debug": "^4.4.0",
    "extract-cbd-shape": "^0.1.11",
    "heap-js": "^2.5.0",
    "n3": "^1.23.1",
    "rdf-data-factory": "^1.1.2",
    "rdf-dereference": "^4.0.0",
    "rdf-lens": "^1.3.0",
    "rdf-stores": "^1.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@fastify/static": "^7.0.4",
    "@rdfc/js-runner": "^1.0.0-alpha.0",
    "@rdfjs/types": "^1.1.2",
    "@types/n3": "^1.21.1",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitest/coverage-v8": "^2.1.8",
    "benchmark": "^2.1.4",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "fastify": "^4.29.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
