{
  "name": "ldes-client",
  "description": "This package provides common tooling to work with LDESes.",
  "version": "0.0.10-alpha.12",
  "main": "dist/lib/client.js",
  "exports": {
    "import": "./dist/lib/client.js",
    "require": "./dist/lib/client.cjs"
  },
  "type": "module",
  "bin": {
    "ldes-client": "dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias && rollup ./dist/lib/client.js --file ./dist/lib/client.cjs --format cjs",
    "test": "vitest run --coverage --coverage.include lib"
  },
  "files": [
    "dist/**/*",
    "shapes/**/*",
    "dist/bin/**/*",
    "processor.ttl"
  ],
  "directories": {
    "lib": "lib",
    "bin": "bin",
    "test": "tests"
  },
  "author": "Arthur Vercruysse",
  "license": "MIT",
  "dependencies": {
    "@treecg/types": "^0.4.6",
    "@types/debug": "^4.1.12",
    "commander": "^11.1.0",
    "debug": "^4.3.4",
    "extract-cbd-shape": "^0.1.5",
    "heap-js": "^2.5.0",
    "n3": "^1.17.3",
    "rdf-data-factory": "^1.1.2",
    "rdf-dereference": "^3.0.0",
    "rdf-lens": "^1.2.8",
    "rdf-stores": "^1.0.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@rdfc/js-runner": "^1.0.0-alpha.0",
    "@rdfjs/types": "^1.1.0",
    "@types/n3": "^1.16.4",
    "@types/node": "^20.12.4",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "benchmark": "^2.1.4",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.4.3",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1",
    "@vitest/coverage-v8": "^2.1.1"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
